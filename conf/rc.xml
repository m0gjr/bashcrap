<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">

<resistance>
	<strength>10</strength>
	<screen_edge_strength>50</screen_edge_strength>
</resistance>

<focus>
	<focusNew>yes</focusNew>
	<followMouse>no</followMouse>
	<focusLast>yes</focusLast>
	<underMouse>no</underMouse>
	<focusDelay>200</focusDelay>
	<raiseOnFocus>no</raiseOnFocus>
</focus>

<placement>
	<policy>Smart</policy>
	<center>no</center>
</placement>

<desktops>
	<number>8</number>
	<firstdesk>1</firstdesk>
	<names>
		<name>1</name>
		<name>2</name>
		<name>3</name>
		<name>4</name>
		<name>5</name>
		<name>6</name>
		<name>7</name>
		<name>8</name>
	</names>
	<popupTime>875</popupTime>
</desktops>

<keyboard>

<keybind key="C-A-Left"><action name="DesktopLeft"></action></keybind>
<keybind key="C-A-Right"><action name="DesktopRight"></action></keybind>
<keybind key="S-A-Left"><action name="SendToDesktopLeft"></action></keybind>
<keybind key="S-A-Right"><action name="SendToDesktopRight"></action></keybind>
<keybind key="W-F1"><action name="Desktop"><desktop>1</desktop></action></keybind>
<keybind key="W-F2"><action name="Desktop"><desktop>2</desktop></action></keybind>
<keybind key="W-F3"><action name="Desktop"><desktop>3</desktop></action></keybind>
<keybind key="W-F4"><action name="Desktop"><desktop>4</desktop></action></keybind>
<keybind key="W-F5"><action name="Desktop"><desktop>5</desktop></action></keybind>
<keybind key="W-F6"><action name="Desktop"><desktop>6</desktop></action></keybind>
<keybind key="W-F7"><action name="Desktop"><desktop>7</desktop></action></keybind>
<keybind key="W-F8"><action name="Desktop"><desktop>8</desktop></action></keybind>

<keybind key="A-F4"><action name="Close"/></keybind>
<keybind key="W-p"><action name="Execute"><execute>scrot</execute></action></keybind>
<keybind key="W-Up"><action name="MaximizeFull"/></keybind>
<keybind key="W-Left"><action name="UnmaximizeFull"/><action name="MaximizeVert"/><action name="MoveResizeTo"><x>0</x><y>0</y><width client="yes">1/2</width></action></keybind>
<keybind key="W-Right"><action name="UnmaximizeFull"/><action name="MaximizeVert"/><action name="MoveResizeTo"><x>-0</x><y>0</y><width client="yes">1/2</width></action></keybind>
<keybind key="W-Down"><action name="UnmaximizeFull"/></keybind>
<keybind key="A-Tab"><action name="NextWindow"/></keybind>

<keybind key="XF86Launch1"><action name="Execute"><execute>xterm</execute></action></keybind>
<keybind key="W-t"><action name="Execute"><command>xterm</command></action></keybind>
<keybind key="C-A-t"><action name="Execute"><command>xterm</command></action></keybind>
<keybind key="W-f"><action name="Execute"><command>files</command></action></keybind>
<keybind key="W-w"><action name="Execute"><command>browser</command></action></keybind>
<keybind key="W-r"><action name="Execute"><command>browser tor</command></action></keybind>
<keybind key="W-g"><action name="Execute"><command>gnumeric</command></action></keybind>
<keybind key="W-v"><action name="Execute"><command>xterm -e alsamixer</command></action></keybind>
<keybind key="W-h"><action name="Execute"><command>xterm -e top</command></action></keybind>
<keybind key="W-c"><action name="Execute"><command>xterm -e bc</command></action></keybind>
<keybind key="W-s"><action name="Execute"><command>xterm -e ssh 10.1</command></action></keybind>
<keybind key="W-d"><action name="Execute"><command>xterm -e watch sensors</command></action></keybind>

<keybind key="XF86MonBrightnessUp"><action name="Execute"><command>brightness up</command></action></keybind>
<keybind key="XF86MonBrightnessDown"><action name="Execute"><command>brightness down</command></action></keybind>
<keybind key="XF86AudioRaiseVolume"><action name="Execute"><command>volume up</command></action></keybind>
<keybind key="XF86AudioLowerVolume"><action name="Execute"><command>volume down</command></action></keybind>
<keybind key="XF86AudioMute"><action name="Execute"><command>volume mute</command></action></keybind>
<keybind key="XF86AudioMicMute"><action name="Execute"><command>volume mic</command></action></keybind>

<keybind key="W-A-Up"><action name="Execute"><execute>screenconf normal</execute></action></keybind>
<keybind key="W-A-Left"><action name="Execute"><execute>screenconf left</execute></action></keybind>
<keybind key="W-A-Right"><action name="Execute"><execute>screenconf right</execute></action></keybind>
<keybind key="W-A-Down"><action name="Execute"><execute>screenconf inverted</execute></action></keybind>

<keybind key="W-x"><action name="Execute"><command>openbox --exit</command></action></keybind>
<keybind key="W-Tab"><action name="ShowMenu"><menu>client-list-combined-menu</menu></action></keybind>

</keyboard>
</openbox_config>
